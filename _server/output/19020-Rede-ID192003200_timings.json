   ]
  }, 
  {
   "begin": "269.400", 
   "children": [], 
   "end": "270.960", 
   "id": "s000089", 
   "language": "deu", 
   "lines": [
    "die uns vorliegt\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "270.960", 
   "children": [], 
   "end": "272.840", 
   "id": "s000090", 
   "language": "deu", 
   "lines": [
    "in den Schutzzonen, "
   ]
