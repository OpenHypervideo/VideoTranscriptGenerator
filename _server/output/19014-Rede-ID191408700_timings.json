  }, 
  {
   "begin": "140.360", 
   "children": [], 
   "end": "141.320", 
   "id": "s000046", 
   "language": "deu", 
   "lines": [
    "Gabriel, "
   ]
  }, 
  {
   "begin": "141.320", 
   "children": [], 
   "end": "143.560", 
   "id": "s000047", 
   "language": "deu", 
   "lines": [
