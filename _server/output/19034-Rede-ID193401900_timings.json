  }, 
  {
   "begin": "9.000", 
   "children": [], 
   "end": "10.040", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "Herr Kollege Lauterbach, "
   ]
  }, 
  {
   "begin": "10.040", 
   "children": [], 
   "end": "11.320", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
