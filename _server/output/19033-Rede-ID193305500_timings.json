  }, 
  {
   "begin": "273.640", 
   "children": [], 
   "end": "274.360", 
   "id": "s000085", 
   "language": "deu", 
   "lines": [
    "Herr M\u00fcnz, "
   ]
  }, 
  {
   "begin": "274.360", 
   "children": [], 
   "end": "282.680", 
   "id": "s000086", 
   "language": "deu", 
   "lines": [
