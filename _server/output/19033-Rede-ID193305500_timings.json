  {
   "begin": "282.680", 
   "children": [], 
   "end": "283.480", 
   "id": "s000087", 
   "language": "deu", 
   "lines": [
    "Die zweite Antwort, "
   ]
  }, 
  {
   "begin": "283.480", 
   "children": [], 
   "end": "286.920", 
   "id": "s000088", 
   "language": "deu", 
   "lines": [
    "die wir als Politik auf diese gesellschaftliche Entwicklung geben, "
   ]
  }, 
  {
   "begin": "286.920", 
   "children": [], 
   "end": "287.240", 
   "id": "s000089", 
   "language": "deu", 
   "lines": [
    "ist, "
   ]
  }, 
  {
   "begin": "287.240", 
   "children": [], 
