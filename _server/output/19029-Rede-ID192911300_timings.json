  {
   "begin": "199.320", 
   "children": [], 
   "end": "200.720", 
   "id": "s000074", 
   "language": "deu", 
   "lines": [
    "Liebe Gro\u00dfkoalition\u00e4re: "
   ]
  }, 
  {
   "begin": "200.720", 
   "children": [], 
   "end": "201.600", 
   "id": "s000075", 
   "language": "deu", 
   "lines": [
    "gut gedacht, "
   ]
  }, 
  {
   "begin": "201.600", 
   "children": [], 
   "end": "202.640", 
   "id": "s000076", 
   "language": "deu", 
   "lines": [
