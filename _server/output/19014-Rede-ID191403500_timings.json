  }, 
  {
   "begin": "112.400", 
   "children": [], 
   "end": "113.640", 
   "id": "s000051", 
   "language": "deu", 
   "lines": [
    "ganz ehrlich\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "113.640", 
   "children": [], 
   "end": "115.520", 
   "id": "s000052", 
   "language": "deu", 
   "lines": [
