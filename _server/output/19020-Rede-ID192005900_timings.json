  }, 
  {
   "begin": "178.360", 
   "children": [], 
   "end": "178.720", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
    "Ja, "
   ]
  }, 
  {
   "begin": "178.720", 
   "children": [], 
   "end": "183.160", 
   "id": "s000059", 
   "language": "deu", 
   "lines": [
    "wir haben unterschiedliche Arten der Vertretung. "
   ]
  }, 
  {
   "begin": "183.160", 
   "children": [], 
   "end": "184.360", 
   "id": "s000060", 
   "language": "deu", 
   "lines": [
    "In gro\u00dfen Konzernen, "
   ]
  }, 
  {
   "begin": "184.360", 
   "children": [], 
   "end": "186.600", 
   "id": "s000061", 
   "language": "deu", 
   "lines": [
