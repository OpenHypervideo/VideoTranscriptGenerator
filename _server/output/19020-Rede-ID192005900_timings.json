  }, 
  {
   "begin": "213.000", 
   "children": [], 
   "end": "213.640", 
   "id": "s000073", 
   "language": "deu", 
   "lines": [
    "Das tun wir, "
   ]
  }, 
  {
   "begin": "213.640", 
   "children": [], 
   "end": "216.080", 
   "id": "s000074", 
   "language": "deu", 
   "lines": [
