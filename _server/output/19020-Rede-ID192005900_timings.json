  }, 
  {
   "begin": "197.080", 
   "children": [], 
   "end": "197.640", 
   "id": "s000067", 
   "language": "deu", 
   "lines": [
    "dass man sich kennt, "
   ]
  }, 
  {
   "begin": "197.640", 
   "children": [], 
   "end": "200.600", 
   "id": "s000068", 
   "language": "deu", 
   "lines": [
