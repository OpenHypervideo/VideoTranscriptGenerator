  }, 
  {
   "begin": "270.960", 
   "children": [], 
   "end": "272.600", 
   "id": "s000081", 
   "language": "deu", 
   "lines": [
    "die arbeitslos werden, "
   ]
  }, 
  {
   "begin": "272.600", 
   "children": [], 
   "end": "275.680", 
   "id": "s000082", 
   "language": "deu", 
   "lines": [
