   ]
  }, 
  {
   "begin": "140.720", 
   "children": [], 
   "end": "142.080", 
   "id": "s000040", 
   "language": "deu", 
   "lines": [
    "in die Infrastruktur, "
   ]
  }, 
  {
   "begin": "142.080", 
   "children": [], 
   "end": "146.440", 
   "id": "s000041", 
   "language": "deu", 
   "lines": [
