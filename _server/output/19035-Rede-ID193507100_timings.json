  }, 
  {
   "begin": "241.120", 
   "children": [], 
   "end": "242.400", 
   "id": "s000069", 
   "language": "deu", 
   "lines": [
    "die Trends erkennt, "
   ]
  }, 
  {
   "begin": "242.400", 
   "children": [], 
   "end": "249.960", 
   "id": "s000070", 
   "language": "deu", 
   "lines": [
