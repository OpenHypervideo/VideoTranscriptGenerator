  }, 
  {
   "begin": "231.280", 
   "children": [], 
   "end": "232.360", 
   "id": "s000064", 
   "language": "deu", 
   "lines": [
    "meine Damen und Herren, "
   ]
  }, 
  {
   "begin": "232.360", 
   "children": [], 
   "end": "233.720", 
   "id": "s000065", 
   "language": "deu", 
   "lines": [
