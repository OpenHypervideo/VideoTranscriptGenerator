   ]
  }, 
  {
   "begin": "36.880", 
   "children": [], 
   "end": "37.520", 
   "id": "s000018", 
   "language": "deu", 
   "lines": [
    "n\u00e4mlich darum: "
   ]
  }, 
  {
   "begin": "37.520", 
   "children": [], 
   "end": "40.960", 
   "id": "s000019", 
   "language": "deu", 
   "lines": [
