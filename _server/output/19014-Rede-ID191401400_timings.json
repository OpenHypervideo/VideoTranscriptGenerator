  {
   "begin": "276.320", 
   "children": [], 
   "end": "277.240", 
   "id": "s000086", 
   "language": "deu", 
   "lines": [
    "Deswegen: "
   ]
  }, 
  {
   "begin": "277.240", 
   "children": [], 
   "end": "278.640", 
   "id": "s000087", 
   "language": "deu", 
   "lines": [
    "Ehe man nach neuem, "
   ]
  }, 
  {
   "begin": "278.640", 
   "children": [], 
   "end": "279.760", 
   "id": "s000088", 
   "language": "deu", 
   "lines": [
    "nach mehr Geld ruft, "
   ]
  }, 
  {
   "begin": "279.760", 
   "children": [], 
   "end": "283.000", 
