   ]
  }, 
  {
   "begin": "509.760", 
   "children": [], 
   "end": "511.360", 
   "id": "s000165", 
   "language": "deu", 
   "lines": [
    "Niels Annen.\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "511.360", 
   "children": [], 
   "end": "512.400", 
