  }, 
  {
   "begin": "225.320", 
   "children": [], 
   "end": "225.720", 
   "id": "s000073", 
   "language": "deu", 
   "lines": [
    "Ich glaube, "
   ]
  }, 
  {
   "begin": "225.720", 
   "children": [], 
   "end": "230.000", 
   "id": "s000074", 
   "language": "deu", 
   "lines": [
