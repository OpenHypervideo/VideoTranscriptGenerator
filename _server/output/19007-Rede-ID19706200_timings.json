  }, 
  {
   "begin": "423.160", 
   "children": [], 
   "end": "425.560", 
   "id": "s000132", 
   "language": "deu", 
   "lines": [
    "tragen beispielsweise die Ausschreibungen, "
   ]
  }, 
  {
   "begin": "425.560", 
   "children": [], 
   "end": "427.400", 
   "id": "s000133", 
   "language": "deu", 
   "lines": [
    "gegen die Sie immer waren, "
   ]
  }, 
  {
   "begin": "427.400", 
   "children": [], 
   "end": "428.160", 
   "id": "s000134", 
   "language": "deu", 
   "lines": [
    "dazu bei, "
   ]
  }, 
  {
   "begin": "428.160", 
   "children": [], 
   "end": "435.000", 
   "id": "s000135", 
   "language": "deu", 
   "lines": [
