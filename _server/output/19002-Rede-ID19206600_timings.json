   ]
  }, 
  {
   "begin": "60.040", 
   "children": [], 
   "end": "62.680", 
   "id": "s000021", 
   "language": "deu", 
   "lines": [
    "90\u00a0000\u00a0Besch\u00e4ftigte. "
   ]
  }, 
  {
   "begin": "62.680", 
   "children": [], 
   "end": "65.600", 
   "id": "s000022", 
   "language": "deu", 
   "lines": [
