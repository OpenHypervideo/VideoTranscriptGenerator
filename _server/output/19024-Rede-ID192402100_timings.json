  }, 
  {
   "begin": "5.160", 
   "children": [], 
   "end": "6.240", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "Ihre Ans\u00e4tze\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "6.240", 
   "children": [], 
   "end": "7.120", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
    "da bin ich einmal bei Ihnen\u00a0\u2013, "
   ]
  }, 
  {
   "begin": "7.120", 
   "children": [], 
   "end": "10.200", 
   "id": "s000005", 
   "language": "deu", 
   "lines": [
    "den Rechtsbereich zu st\u00e4rken und Personal einzustellen, "
   ]
  }, 
  {
   "begin": "10.200", 
   "children": [], 
   "end": "11.360", 
   "id": "s000006", 
   "language": "deu", 
   "lines": [
