{
 "fragments": [
  {
   "begin": "0.000", 
   "children": [], 
   "end": "10.480", 
   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Sehr geehrte Frau Pr\u00e4sidentin! "
   ]
  }, 
  {
   "begin": "10.480", 
   "children": [], 
   "end": "14.840", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
