   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Herr Pr\u00e4sident! "
   ]
  }, 
  {
   "begin": "6.160", 
   "children": [], 
   "end": "7.360", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
    "Liebe Kolleginnen und Kollegen! "
   ]
  }, 
  {
   "begin": "7.360", 
   "children": [], 
   "end": "8.200", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
