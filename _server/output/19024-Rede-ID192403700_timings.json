  }, 
  {
   "begin": "204.920", 
   "children": [], 
   "end": "206.120", 
   "id": "s000078", 
   "language": "deu", 
   "lines": [
    "sondern trotz der Politik, "
   ]
  }, 
  {
   "begin": "206.120", 
   "children": [], 
   "end": "206.920", 
   "id": "s000079", 
   "language": "deu", 
   "lines": [
