  }, 
  {
   "begin": "225.320", 
   "children": [], 
   "end": "226.120", 
   "id": "s000092", 
   "language": "deu", 
   "lines": [
    "schneller Ausbau der Erneuerbaren, "
   ]
  }, 
  {
   "begin": "226.120", 
   "children": [], 
   "end": "226.440", 
   "id": "s000093", 
   "language": "deu", 
   "lines": [
    "Klimaschutzgesetz\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "226.440", 
   "children": [], 
   "end": "227.200", 
   "id": "s000094", 
   "language": "deu", 
   "lines": [
