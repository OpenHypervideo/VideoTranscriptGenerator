  }, 
  {
   "begin": "693.640", 
   "children": [], 
   "end": "695.720", 
   "id": "s000240", 
   "language": "deu", 
   "lines": [
    "dass einer der drei, "
   ]
  }, 
  {
   "begin": "695.720", 
   "children": [], 
   "end": "697.600", 
   "id": "s000241", 
   "language": "deu", 
   "lines": [
