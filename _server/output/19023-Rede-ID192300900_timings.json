  }, 
  {
   "begin": "685.240", 
   "children": [], 
   "end": "686.280", 
   "id": "s000235", 
   "language": "deu", 
   "lines": [
    "13\u00a0Jahre, "
   ]
  }, 
  {
   "begin": "686.280", 
   "children": [], 
   "end": "687.280", 
   "id": "s000236", 
   "language": "deu", 
   "lines": [
