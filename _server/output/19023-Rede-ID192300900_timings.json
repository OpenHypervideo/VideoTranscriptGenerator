  }, 
  {
   "begin": "698.280", 
   "children": [], 
   "end": "698.840", 
   "id": "s000243", 
   "language": "deu", 
   "lines": [
    "Opa, "
   ]
  }, 
  {
   "begin": "698.840", 
   "children": [], 
   "end": "700.280", 
   "id": "s000244", 
   "language": "deu", 
   "lines": [
    "du warst doch Haush\u00e4lter. "
   ]
  }, 
  {
   "begin": "702.600", 
   "children": [], 
   "end": "704.080", 
   "id": "s000245", 
   "language": "deu", 
   "lines": [
