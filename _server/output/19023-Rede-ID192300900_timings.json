  }, 
  {
   "begin": "714.000", 
   "children": [], 
   "end": "715.280", 
   "id": "s000249", 
   "language": "deu", 
   "lines": [
    "hei\u00dft Nachhaltigkeit. "
   ]
  }, 
  {
   "begin": "715.280", 
   "children": [], 
   "end": "717.440", 
   "id": "s000250", 
   "language": "deu", 
   "lines": [
    "Es hei\u00dft auch Generationengerechtigkeit. "
   ]
  }, 
  {
   "begin": "717.440", 
   "children": [], 
   "end": "733.320", 
   "id": "s000251", 
   "language": "deu", 
   "lines": [
