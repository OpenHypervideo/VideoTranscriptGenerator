  }, 
  {
   "begin": "111.240", 
   "children": [], 
   "end": "112.400", 
   "id": "s000032", 
   "language": "deu", 
   "lines": [
    "Herr Kollege D\u00fcrr, "
   ]
  }, 
  {
   "begin": "112.400", 
   "children": [], 
   "end": "114.000", 
   "id": "s000033", 
   "language": "deu", 
   "lines": [
