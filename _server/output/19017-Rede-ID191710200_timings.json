  }, 
  {
   "begin": "42.800", 
   "children": [], 
   "end": "43.640", 
   "id": "s000017", 
   "language": "deu", 
   "lines": [
    "Der BAf\u00f6G-Bericht\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "43.640", 
   "children": [], 
   "end": "45.160", 
   "id": "s000018", 
   "language": "deu", 
   "lines": [
