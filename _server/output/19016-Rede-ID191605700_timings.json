  }, 
  {
   "begin": "220.120", 
   "children": [], 
   "end": "220.640", 
   "id": "s000083", 
   "language": "deu", 
   "lines": [
    "Ich glaube, "
   ]
  }, 
  {
   "begin": "220.640", 
   "children": [], 
   "end": "224.360", 
   "id": "s000084", 
   "language": "deu", 
   "lines": [
    "es war Ihr damaliger Parteivorsitzender Cem \u00d6zdemir, "
   ]
  }, 
  {
   "begin": "224.360", 
   "children": [], 
   "end": "224.880", 
   "id": "s000085", 
   "language": "deu", 
   "lines": [
