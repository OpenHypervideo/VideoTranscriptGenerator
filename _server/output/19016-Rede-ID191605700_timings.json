  }, 
  {
   "begin": "238.600", 
   "children": [], 
   "end": "239.400", 
   "id": "s000089", 
   "language": "deu", 
   "lines": [
    "dass wir denen, "
   ]
  }, 
  {
   "begin": "239.400", 
   "children": [], 
   "end": "241.800", 
   "id": "s000090", 
   "language": "deu", 
   "lines": [
    "die zur Tat bereit waren, "
   ]
  }, 
  {
   "begin": "241.800", 
   "children": [], 
   "end": "244.280", 
   "id": "s000091", 
   "language": "deu", 
