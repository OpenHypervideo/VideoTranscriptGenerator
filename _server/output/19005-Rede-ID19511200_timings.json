   "end": "3.160", 
   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Frau Pr\u00e4sidentin! "
   ]
  }, 
  {
   "begin": "3.160", 
   "children": [], 
   "end": "8.880", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
    "Meine Damen und Herren! "
   ]
  }, 
  {
   "begin": "8.880", 
   "children": [], 
   "end": "9.400", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "Die Debatte, "
   ]
  }, 
  {
   "begin": "9.400", 
   "children": [], 
   "end": "12.720", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
