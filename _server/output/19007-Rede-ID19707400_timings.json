  {
   "begin": "264.920", 
   "children": [], 
   "end": "266.320", 
   "id": "s000093", 
   "language": "deu", 
   "lines": [
    "Das erinnert daran. "
   ]
  }, 
  {
   "begin": "266.320", 
   "children": [], 
   "end": "269.120", 
   "id": "s000094", 
   "language": "deu", 
   "lines": [
