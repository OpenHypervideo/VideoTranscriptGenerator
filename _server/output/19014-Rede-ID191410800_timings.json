  {
   "begin": "170.720", 
   "children": [], 
   "end": "171.560", 
   "id": "s000057", 
   "language": "deu", 
   "lines": [
    "Eine Antwort, "
   ]
  }, 
  {
   "begin": "171.560", 
   "children": [], 
   "end": "172.320", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
    "die gegeben wird, "
   ]
  }, 
  {
   "begin": "172.320", 
   "children": [], 
   "end": "172.720", 
   "id": "s000059", 
   "language": "deu", 
   "lines": [
    "ist, "
   ]
  }, 
  {
   "begin": "172.720", 
   "children": [], 
   "end": "175.280", 
   "id": "s000060", 
   "language": "deu", 
   "lines": [
