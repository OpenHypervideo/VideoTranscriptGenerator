   ]
  }, 
  {
   "begin": "96.640", 
   "children": [], 
   "end": "97.680", 
   "id": "s000037", 
   "language": "deu", 
   "lines": [
    "die gerne mehr\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "97.680", 
   "children": [], 
   "end": "98.440", 
   "id": "s000038", 
   "language": "deu", 
   "lines": [
    "oder \u00fcberhaupt\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "98.440", 
   "children": [], 
   "end": "99.400", 
   "id": "s000039", 
   "language": "deu", 
   "lines": [
    "arbeiten m\u00f6chten, "
   ]
  }, 
  {
   "begin": "99.400", 
   "children": [], 
   "end": "101.880", 
   "id": "s000040", 
   "language": "deu", 
   "lines": [
