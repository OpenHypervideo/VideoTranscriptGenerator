  }, 
  {
   "begin": "236.240", 
   "children": [], 
   "end": "237.160", 
   "id": "s000083", 
   "language": "deu", 
   "lines": [
    "Jawohl. "
   ]
  }, 
  {
   "begin": "237.160", 
   "children": [], 
   "end": "239.920", 
   "id": "s000084", 
   "language": "deu", 
   "lines": [
