   "begin": "149.960", 
   "children": [], 
   "end": "154.080", 
   "id": "s000053", 
   "language": "deu", 
   "lines": [
    "der eben undeutlich an mein Ohr drang\u00a0\u2013\u00a0\u2013"
   ]
  }, 
  {
   "begin": "154.080", 
   "children": [], 
   "end": "154.480", 
   "id": "s000054", 
   "language": "deu", 
   "lines": [
    "\u2013\u00a0Gut, "
   ]
  }, 
  {
   "begin": "154.480", 
   "children": [], 
   "end": "155.600", 
   "id": "s000055", 
   "language": "deu", 
   "lines": [
    "dann ist er hier gefallen.\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "155.600", 
   "children": [], 
   "end": "155.760", 
   "id": "s000056", 
   "language": "deu", 
   "lines": [
