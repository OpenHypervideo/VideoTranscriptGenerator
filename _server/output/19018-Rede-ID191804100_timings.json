   "end": "2.600", 
   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Herr Pr\u00e4sident! "
   ]
  }, 
  {
   "begin": "2.600", 
   "children": [], 
   "end": "8.200", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
