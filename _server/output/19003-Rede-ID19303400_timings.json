  }, 
  {
   "begin": "28.720", 
   "children": [], 
   "end": "29.920", 
   "id": "s000011", 
   "language": "deu", 
   "lines": [
    "Ich freue mich\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "29.920", 
   "children": [], 
   "end": "39.800", 
   "id": "s000012", 
   "language": "deu", 
   "lines": [
