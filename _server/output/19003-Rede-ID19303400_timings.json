   ]
  }, 
  {
   "begin": "145.360", 
   "children": [], 
   "end": "147.760", 
   "id": "s000054", 
   "language": "deu", 
   "lines": [
    "und zwar f\u00fcr alle Beteiligten:"
   ]
  }, 
  {
   "begin": "147.760", 
   "children": [], 
   "end": "149.240", 
   "id": "s000055", 
   "language": "deu", 
   "lines": [
    "f\u00fcr die Herkunftsstaaten, "
   ]
  }, 
  {
   "begin": "149.240", 
   "children": [], 
   "end": "155.080", 
   "id": "s000056", 
   "language": "deu", 
   "lines": [
