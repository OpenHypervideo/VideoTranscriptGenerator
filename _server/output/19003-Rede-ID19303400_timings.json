   ]
  }, 
  {
   "begin": "203.280", 
   "children": [], 
   "end": "204.920", 
   "id": "s000076", 
   "language": "deu", 
   "lines": [
    "einen Gesetzentwurf eingebracht\u00a0\u2013, "
   ]
  }, 
  {
   "begin": "204.920", 
   "children": [], 
   "end": "206.360", 
   "id": "s000077", 
   "language": "deu", 
   "lines": [
    "neben das bisher geltende Recht, "
   ]
  }, 
  {
   "begin": "206.360", 
   "children": [], 
   "end": "209.920", 
   "id": "s000078", 
   "language": "deu", 
   "lines": [
    "das die Regelungen zur Arbeitsmigration eben nicht vereinfacht, "
   ]
  }, 
  {
   "begin": "209.920", 
   "children": [], 
   "end": "212.680", 
   "id": "s000079", 
   "language": "deu", 
   "lines": [
