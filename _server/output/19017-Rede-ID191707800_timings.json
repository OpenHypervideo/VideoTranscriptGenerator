  }, 
  {
   "begin": "337.640", 
   "children": [], 
   "end": "338.000", 
   "id": "s000123", 
   "language": "deu", 
   "lines": [
    "Ja, "
   ]
  }, 
  {
   "begin": "338.000", 
   "children": [], 
   "end": "339.120", 
   "id": "s000124", 
   "language": "deu", 
   "lines": [
    "k\u00fcmmert euch um die Probleme, "
   ]
  }, 
  {
   "begin": "339.120", 
   "children": [], 
   "end": "339.200", 
   "id": "s000125", 
   "language": "deu", 
   "lines": [
