  }, 
  {
   "begin": "102.720", 
   "children": [], 
   "end": "103.640", 
   "id": "s000032", 
   "language": "deu", 
   "lines": [
    "Ich wei\u00df, "
   ]
  }, 
  {
   "begin": "103.640", 
   "children": [], 
   "end": "105.720", 
   "id": "s000033", 
   "language": "deu", 
   "lines": [
