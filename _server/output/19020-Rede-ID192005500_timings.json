  {
   "begin": "16.120", 
   "children": [], 
   "end": "17.680", 
   "id": "s000005", 
   "language": "deu", 
   "lines": [
    "er hat ungef\u00e4hr 20\u00a0Mitarbeiter\u00a0\u2013 "
