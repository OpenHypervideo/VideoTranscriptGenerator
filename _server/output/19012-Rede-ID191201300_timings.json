  }, 
  {
   "begin": "136.000", 
   "children": [], 
   "end": "137.680", 
   "id": "s000040", 
   "language": "deu", 
   "lines": [
    "H\u00f6ren Sie auf, "
   ]
  }, 
  {
   "begin": "137.680", 
   "children": [], 
   "end": "141.040", 
   "id": "s000041", 
   "language": "deu", 
   "lines": [
