  }, 
  {
   "begin": "75.240", 
   "children": [], 
   "end": "75.920", 
   "id": "s000021", 
   "language": "deu", 
   "lines": [
    "Die Debatte, "
   ]
  }, 
  {
   "begin": "75.920", 
   "children": [], 
   "end": "77.360", 
   "id": "s000022", 
   "language": "deu", 
   "lines": [
