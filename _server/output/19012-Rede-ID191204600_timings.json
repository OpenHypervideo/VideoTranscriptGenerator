  {
   "begin": "524.120", 
   "children": [], 
   "end": "526.560", 
   "id": "s000129", 
   "language": "deu", 
   "lines": [
    "Aber es ist wichtig, "
   ]
  }, 
  {
   "begin": "526.560", 
   "children": [], 
   "end": "531.520", 
   "id": "s000130", 
   "language": "deu", 
   "lines": [
