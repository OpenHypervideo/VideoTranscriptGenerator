  }, 
  {
   "begin": "523.160", 
   "children": [], 
   "end": "524.080", 
   "id": "s000144", 
   "language": "deu", 
   "lines": [
    "wenn man genauer hinsieht, "
   ]
  }, 
  {
   "begin": "524.080", 
   "children": [], 
   "end": "524.560", 
   "id": "s000145", 
   "language": "deu", 
   "lines": [
    "merkt man es\u00a0\u2013 "
