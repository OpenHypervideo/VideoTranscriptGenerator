  }, 
  {
   "begin": "622.760", 
   "children": [], 
   "end": "623.480", 
   "id": "s000183", 
   "language": "deu", 
   "lines": [
    "Liebe Kolleginnen, "
   ]
  }, 
  {
   "begin": "623.480", 
   "children": [], 
   "end": "624.200", 
   "id": "s000184", 
   "language": "deu", 
   "lines": [
    "liebe Kollegen, "
   ]
  }, 
  {
   "begin": "624.200", 
   "children": [], 
   "end": "626.000", 
   "id": "s000185", 
