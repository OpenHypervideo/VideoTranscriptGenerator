  }, 
  {
   "begin": "395.400", 
   "children": [], 
   "end": "397.000", 
   "id": "s000106", 
   "language": "deu", 
   "lines": [
    "um zu sehen, "
   ]
  }, 
  {
   "begin": "397.000", 
   "children": [], 
   "end": "399.520", 
   "id": "s000107", 
   "language": "deu", 
   "lines": [
