  {
   "begin": "583.680", 
   "children": [], 
   "end": "587.520", 
   "id": "s000169", 
   "language": "deu", 
   "lines": [
    "Wenn die EU-Kommission dann sagt: "
   ]
  }, 
  {
   "begin": "587.520", 
   "children": [], 
   "end": "590.720", 
   "id": "s000170", 
   "language": "deu", 
   "lines": [
