  }, 
  {
   "begin": "184.160", 
   "children": [], 
   "end": "186.120", 
   "id": "s000057", 
   "language": "deu", 
   "lines": [
    "dass immer noch behauptet wird, "
   ]
  }, 
  {
   "begin": "186.120", 
   "children": [], 
   "end": "191.000", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
