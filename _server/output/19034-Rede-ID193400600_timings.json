  }, 
  {
   "begin": "271.760", 
   "children": [], 
   "end": "272.920", 
   "id": "s000105", 
   "language": "deu", 
   "lines": [
    "Sie von der CSU, "
   ]
  }, 
  {
   "begin": "272.920", 
   "children": [], 
   "end": "277.520", 
   "id": "s000106", 
   "language": "deu", 
   "lines": [
