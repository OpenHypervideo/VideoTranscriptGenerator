  }, 
  {
   "begin": "166.680", 
   "children": [], 
   "end": "169.360", 
   "id": "s000057", 
   "language": "deu", 
   "lines": [
    "die toll integrierten libanesischen Familienclans\u00a0\u2013, "
   ]
  }, 
  {
   "begin": "169.360", 
   "children": [], 
   "end": "170.760", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
    "keine kulturfremden, "
   ]
  }, 
  {
   "begin": "170.760", 
   "children": [], 
   "end": "174.280", 
   "id": "s000059", 
   "language": "deu", 
