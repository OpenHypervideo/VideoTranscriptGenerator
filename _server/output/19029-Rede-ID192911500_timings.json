  }, 
  {
   "begin": "161.440", 
   "children": [], 
   "end": "162.760", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
    "weil man glaubt, "
   ]
  }, 
  {
   "begin": "162.760", 
   "children": [], 
   "end": "167.440", 
   "id": "s000059", 
   "language": "deu", 
   "lines": [
