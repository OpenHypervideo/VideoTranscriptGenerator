  }, 
  {
   "begin": "87.240", 
   "children": [], 
   "end": "88.440", 
   "id": "s000027", 
   "language": "deu", 
   "lines": [
    "in der er mir sagt, "
   ]
  }, 
  {
   "begin": "88.440", 
   "children": [], 
   "end": "90.600", 
   "id": "s000028", 
   "language": "deu", 
   "lines": [
