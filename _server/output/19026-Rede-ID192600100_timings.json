   ]
  }, 
  {
   "begin": "331.880", 
   "children": [], 
   "end": "332.960", 
   "id": "s000097", 
   "language": "deu", 
   "lines": [
    "das Erreichte\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "332.960", 
   "children": [], 
   "end": "334.040", 
   "id": "s000098", 
   "language": "deu", 
   "lines": [
    "das ist nicht wenig\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "334.040", 
   "children": [], 
   "end": "335.120", 
   "id": "s000099", 
   "language": "deu", 
   "lines": [
