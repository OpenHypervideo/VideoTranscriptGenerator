   ]
  }, 
  {
   "begin": "227.200", 
   "children": [], 
   "end": "228.560", 
   "id": "s000089", 
   "language": "deu", 
   "lines": [
    "Herr Kollege Kober, "
   ]
  }, 
  {
   "begin": "228.560", 
   "children": [], 
   "end": "229.520", 
   "id": "s000090", 
   "language": "deu", 
   "lines": [
    "vielen Dank.\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "229.520", 
   "children": [], 
   "end": "235.960", 
   "id": "s000091", 
