   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Herr Pr\u00e4sident! "
   ]
  }, 
  {
   "begin": "5.320", 
   "children": [], 
   "end": "6.480", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
