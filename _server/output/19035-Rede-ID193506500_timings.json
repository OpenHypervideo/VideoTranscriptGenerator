   ]
  }, 
  {
   "begin": "88.040", 
   "children": [], 
   "end": "89.440", 
   "id": "s000035", 
   "language": "deu", 
   "lines": [
    "ist unverantwortlich; "
   ]
  }, 
  {
   "begin": "89.440", 
   "children": [], 
   "end": "91.520", 
   "id": "s000036", 
   "language": "deu", 
   "lines": [
    "denn alle Generationen\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "91.520", 
   "children": [], 
   "end": "92.400", 
   "id": "s000037", 
   "language": "deu", 
   "lines": [
