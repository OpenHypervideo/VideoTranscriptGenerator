  }, 
  {
   "begin": "108.160", 
   "children": [], 
   "end": "109.080", 
   "id": "s000046", 
   "language": "deu", 
   "lines": [
    "Kommen wir\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "109.080", 
   "children": [], 
   "end": "110.400", 
   "id": "s000047", 
   "language": "deu", 
   "lines": [
    "erster Punkt\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "110.400", 
   "children": [], 
   "end": "113.240", 
   "id": "s000048", 
   "language": "deu", 
   "lines": [
