   "begin": "148.440", 
   "children": [], 
   "end": "152.000", 
   "id": "s000067", 
   "language": "deu", 
   "lines": [
    "auch noch das Thema Befristungen angehen wollen, "
   ]
  }, 
  {
   "begin": "152.000", 
   "children": [], 
   "end": "154.160", 
   "id": "s000068", 
   "language": "deu", 
   "lines": [
