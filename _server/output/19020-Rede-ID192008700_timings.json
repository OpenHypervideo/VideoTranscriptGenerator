   ]
  }, 
  {
   "begin": "62.600", 
   "children": [], 
   "end": "64.280", 
   "id": "s000026", 
   "language": "deu", 
   "lines": [
    "unserem Auftrag nach, "
   ]
  }, 
  {
   "begin": "64.280", 
   "children": [], 
   "end": "66.080", 
   "id": "s000027", 
   "language": "deu", 
   "lines": [
