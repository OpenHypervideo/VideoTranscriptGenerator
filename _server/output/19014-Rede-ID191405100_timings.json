   ]
  }, 
  {
   "begin": "503.000", 
   "children": [], 
   "end": "505.080", 
   "id": "s000183", 
   "language": "deu", 
   "lines": [
    "sondern lieber richtige Energiepolitik machen.\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "505.080", 
   "children": [], 
   "end": "506.920", 
   "id": "s000184", 
   "language": "deu", 
   "lines": [
