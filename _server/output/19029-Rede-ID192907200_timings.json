   ]
  }, 
  {
   "begin": "74.880", 
   "children": [], 
   "end": "76.480", 
   "id": "s000021", 
   "language": "deu", 
   "lines": [
    "in die Debatte einbezogen werden, "
   ]
  }, 
  {
   "begin": "76.480", 
   "children": [], 
   "end": "79.600", 
   "id": "s000022", 
   "language": "deu", 
   "lines": [
