  }, 
  {
   "begin": "326.880", 
   "children": [], 
   "end": "328.200", 
   "id": "s000132", 
   "language": "deu", 
   "lines": [
    "Wir m\u00fcssen auch bedenken, "
   ]
  }, 
  {
   "begin": "328.200", 
   "children": [], 
   "end": "329.040", 
   "id": "s000133", 
   "language": "deu", 
   "lines": [
