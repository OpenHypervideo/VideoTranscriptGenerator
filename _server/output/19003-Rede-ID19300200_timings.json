  }, 
  {
   "begin": "185.480", 
   "children": [], 
   "end": "185.960", 
   "id": "s000057", 
   "language": "deu", 
   "lines": [
    "Ich glaube, "
   ]
  }, 
  {
   "begin": "185.960", 
   "children": [], 
   "end": "199.160", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
