  }, 
  {
   "begin": "9.760", 
   "children": [], 
   "end": "10.800", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "Mali\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "10.800", 
   "children": [], 
   "end": "11.520", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
    "das kann man, "
