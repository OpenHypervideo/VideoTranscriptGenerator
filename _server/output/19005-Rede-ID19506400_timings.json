   ]
  }, 
  {
   "begin": "220.840", 
   "children": [], 
   "end": "221.960", 
   "id": "s000062", 
   "language": "deu", 
   "lines": [
    "mit einem Schriftzug, "
   ]
  }, 
  {
   "begin": "221.960", 
   "children": [], 
   "end": "223.320", 
   "id": "s000063", 
   "language": "deu", 
   "lines": [
    "der so ekelhaft ist, "
   ]
  }, 
  {
   "begin": "223.320", 
   "children": [], 
   "end": "225.560", 
   "id": "s000064", 
   "language": "deu", 
   "lines": [
