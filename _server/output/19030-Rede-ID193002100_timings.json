   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Sehr geehrter Herr Pr\u00e4sident! "
   ]
  }, 
  {
   "begin": "6.240", 
   "children": [], 
   "end": "7.720", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
    "Liebe Kolleginnen und Kollegen! "
   ]
  }, 
  {
   "begin": "7.720", 
   "children": [], 
   "end": "9.160", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "\u201eWenn es nicht notwendig ist, "
   ]
  }, 
  {
   "begin": "9.160", 
   "children": [], 
   "end": "10.760", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
    "ein Gesetz zu machen, "
   ]
  }, 
  {
   "begin": "10.760", 
   "children": [], 
   "end": "12.000", 
   "id": "s000005", 
   "language": "deu", 
   "lines": [
