   ]
  }, 
  {
   "begin": "74.560", 
   "children": [], 
   "end": "76.040", 
   "id": "s000027", 
   "language": "deu", 
   "lines": [
    "selbst in internen Mails\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "76.040", 
   "children": [], 
   "end": "78.080", 
   "id": "s000028", 
   "language": "deu", 
   "lines": [
    "dar\u00fcber ist berichtet worden\u00a0\u2013, "
   ]
  }, 
  {
   "begin": "78.080", 
   "children": [], 
   "end": "83.800", 
   "id": "s000029", 
   "language": "deu", 
   "lines": [
