   "begin": "191.280", 
   "children": [], 
   "end": "192.000", 
   "id": "s000072", 
   "language": "deu", 
   "lines": [
    "Das geht nicht, "
   ]
  }, 
  {
   "begin": "192.000", 
   "children": [], 
   "end": "195.680", 
   "id": "s000073", 
   "language": "deu", 
   "lines": [
