  }, 
  {
   "begin": "43.920", 
   "children": [], 
   "end": "44.320", 
   "id": "s000017", 
   "language": "deu", 
   "lines": [
    "je nachdem, "
   ]
  }, 
  {
   "begin": "44.320", 
   "children": [], 
   "end": "45.960", 
   "id": "s000018", 
   "language": "deu", 
   "lines": [
    "wo wir uns befinden\u00a0\u2013, "
   ]
  }, 
  {
   "begin": "45.960", 
   "children": [], 
   "end": "47.880", 
   "id": "s000019", 
   "language": "deu", 
   "lines": [
