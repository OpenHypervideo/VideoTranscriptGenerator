  {
   "begin": "203.600", 
   "children": [], 
   "end": "205.000", 
   "id": "s000081", 
   "language": "deu", 
   "lines": [
    "Liebe Kolleginnen und Kollegen, "
   ]
  }, 
  {
   "begin": "205.000", 
   "children": [], 
   "end": "205.720", 
   "id": "s000082", 
   "language": "deu", 
   "lines": [
