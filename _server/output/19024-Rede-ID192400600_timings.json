  {
   "begin": "278.880", 
   "children": [], 
   "end": "280.560", 
   "id": "s000090", 
   "language": "deu", 
   "lines": [
    "statt sie zu verteidigen, "
   ]
  }, 
  {
   "begin": "280.560", 
   "children": [], 
   "end": "283.760", 
   "id": "s000091", 
