  }, 
  {
   "begin": "266.680", 
   "children": [], 
   "end": "270.040", 
   "id": "s000087", 
   "language": "deu", 
   "lines": [
    "Erstens. "
   ]
  }, 
  {
   "begin": "270.040", 
   "children": [], 
   "end": "274.680", 
   "id": "s000088", 
   "language": "deu", 
   "lines": [
