   ]
  }, 
  {
   "begin": "83.920", 
   "children": [], 
   "end": "85.280", 
   "id": "s000023", 
   "language": "deu", 
   "lines": [
    "berufst\u00e4tig zu sein\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "85.280", 
   "children": [], 
   "end": "87.240", 
   "id": "s000024", 
   "language": "deu", 
   "lines": [
