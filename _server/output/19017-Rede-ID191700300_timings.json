   ]
  }, 
  {
   "begin": "139.120", 
   "children": [], 
   "end": "140.400", 
   "id": "s000036", 
   "language": "deu", 
   "lines": [
    "nein Jahrzehnte\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "140.400", 
   "children": [], 
   "end": "142.160", 
   "id": "s000037", 
   "language": "deu", 
   "lines": [
