  }, 
  {
   "begin": "272.600", 
   "children": [], 
   "end": "273.760", 
   "id": "s000090", 
   "language": "deu", 
   "lines": [
    "was Sie nicht verstehen, "
   ]
  }, 
  {
   "begin": "273.760", 
   "children": [], 
   "end": "274.080", 
   "id": "s000091", 
   "language": "deu", 
   "lines": [
    "ist, "
   ]
  }, 
  {
   "begin": "274.080", 
   "children": [], 
   "end": "276.880", 
   "id": "s000092", 
   "language": "deu", 
   "lines": [
