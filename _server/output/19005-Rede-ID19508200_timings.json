  }, 
  {
   "begin": "244.920", 
   "children": [], 
   "end": "247.960", 
   "id": "s000082", 
   "language": "deu", 
   "lines": [
    "station\u00e4re Grenz\u00fcbertrittskontrollen durchf\u00fchren, "
   ]
  }, 
  {
   "begin": "247.960", 
   "children": [], 
   "end": "250.320", 
   "id": "s000083", 
   "language": "deu", 
   "lines": [
