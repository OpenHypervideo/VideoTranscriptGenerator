   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Sehr geehrter Herr Pr\u00e4sident! "
   ]
  }, 
  {
   "begin": "7.400", 
   "children": [], 
   "end": "13.840", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
