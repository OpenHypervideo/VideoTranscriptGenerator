  }, 
  {
   "begin": "225.120", 
   "children": [], 
   "end": "226.440", 
   "id": "s000078", 
   "language": "deu", 
   "lines": [
    "Verteidigung, "
   ]
  }, 
  {
   "begin": "226.440", 
   "children": [], 
   "end": "232.800", 
   "id": "s000079", 
   "language": "deu", 
   "lines": [
