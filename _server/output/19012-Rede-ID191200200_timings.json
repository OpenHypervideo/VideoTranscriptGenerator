  }, 
  {
   "begin": "226.880", 
   "children": [], 
   "end": "227.520", 
   "id": "s000064", 
   "language": "deu", 
   "lines": [
    "Das, "
   ]
  }, 
  {
   "begin": "227.520", 
   "children": [], 
   "end": "229.640", 
   "id": "s000065", 
   "language": "deu", 
   "lines": [
