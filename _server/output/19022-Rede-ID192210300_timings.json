   ]
  }, 
  {
   "begin": "56.520", 
   "children": [], 
   "end": "56.920", 
   "id": "s000032", 
   "language": "deu", 
   "lines": [
    "aber auch, "
   ]
  }, 
  {
   "begin": "56.920", 
   "children": [], 
   "end": "58.920", 
   "id": "s000033", 
   "language": "deu", 
   "lines": [
    "an die Rede Ihres Parteifreundes, "
   ]
  }, 
  {
   "begin": "58.920", 
   "children": [], 
   "end": "59.560", 
   "id": "s000034", 
   "language": "deu", 
   "lines": [
    "Herr Felgentreu, "
   ]
  }, 
  {
   "begin": "59.560", 
   "children": [], 
   "end": "62.520", 
   "id": "s000035", 
   "language": "deu", 
   "lines": [
