  }, 
  {
   "begin": "325.560", 
   "children": [], 
   "end": "326.760", 
   "id": "s000136", 
   "language": "deu", 
   "lines": [
    "nicht gespart wird, "
   ]
  }, 
  {
   "begin": "326.760", 
   "children": [], 
   "end": "329.440", 
   "id": "s000137", 
   "language": "deu", 
   "lines": [
    "sondern dass das Geld genau dorthin gelenkt wird. "
   ]
  }, 
  {
   "begin": "329.440", 
   "children": [], 
   "end": "329.640", 
   "id": "s000138", 
   "language": "deu", 
   "lines": [
    "Ich glaube, "
   ]
  }, 
  {
   "begin": "329.640", 
   "children": [], 
   "end": "334.400", 
   "id": "s000139", 
   "language": "deu", 
   "lines": [
