   ]
  }, 
  {
   "begin": "232.480", 
   "children": [], 
   "end": "233.760", 
   "id": "s000071", 
   "language": "deu", 
   "lines": [
    "den Auftrag, "
   ]
  }, 
  {
   "begin": "233.760", 
   "children": [], 
   "end": "236.960", 
   "id": "s000072", 
   "language": "deu", 
   "lines": [
