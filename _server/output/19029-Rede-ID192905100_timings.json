  }, 
  {
   "begin": "219.240", 
   "children": [], 
   "end": "223.080", 
   "id": "s000066", 
   "language": "deu", 
   "lines": [
    "Erst am 21.\u00a0Dezember\u00a02017\u00a0\u2013 "
