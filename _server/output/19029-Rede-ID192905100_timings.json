   ]
  }, 
  {
   "begin": "268.000", 
   "children": [], 
   "end": "270.800", 
   "id": "s000086", 
   "language": "deu", 
   "lines": [
    "F\u00fcnfte Ungereimtheit\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "270.800", 
   "children": [], 
   "end": "273.560", 
   "id": "s000087", 
   "language": "deu", 
   "lines": [
    "ich k\u00f6nnte noch weitere aufz\u00e4hlen, "
   ]
  }, 
  {
   "begin": "273.560", 
   "children": [], 
   "end": "275.240", 
   "id": "s000088", 
   "language": "deu", 
   "lines": [
    "aber die Zeit l\u00e4uft mir davon\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "275.240", 
   "children": [], 
   "end": "275.560", 
   "id": "s000089", 
   "language": "deu", 
   "lines": [
    "ist: "
   ]
  }, 
  {
   "begin": "275.560", 
   "children": [], 
