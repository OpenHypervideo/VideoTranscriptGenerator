   ]
  }, 
  {
   "begin": "204.640", 
   "children": [], 
   "end": "206.360", 
   "id": "s000069", 
   "language": "deu", 
   "lines": [
    "Soll Klein-Anatolien\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "206.360", 
   "children": [], 
   "end": "207.400", 
   "id": "s000070", 
   "language": "deu", 
   "lines": [
    "Berlin-Neuk\u00f6lln\u00a0\u2013, "
   ]
  }, 
  {
   "begin": "207.400", 
   "children": [], 
   "end": "208.160", 
   "id": "s000071", 
   "language": "deu", 
   "lines": [
    "soll Syrien, "
   ]
  }, 
  {
   "begin": "208.160", 
   "children": [], 
   "end": "210.720", 
   "id": "s000072", 
   "language": "deu", 
   "lines": [
