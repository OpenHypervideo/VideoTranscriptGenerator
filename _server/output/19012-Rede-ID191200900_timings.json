   ]
  }, 
  {
   "begin": "168.200", 
   "children": [], 
   "end": "168.920", 
   "id": "s000052", 
   "language": "deu", 
   "lines": [
    "wenn sie erkl\u00e4rt, "
   ]
  }, 
  {
   "begin": "168.920", 
   "children": [], 
   "end": "173.000", 
   "id": "s000053", 
   "language": "deu", 
   "lines": [
