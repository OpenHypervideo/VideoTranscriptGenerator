   "begin": "259.600", 
   "children": [], 
   "end": "261.840", 
   "id": "s000079", 
   "language": "deu", 
   "lines": [
    "dass wir am Ende bei dieser Beschleunigung, "
   ]
  }, 
  {
   "begin": "261.840", 
   "children": [], 
   "end": "263.000", 
   "id": "s000080", 
   "language": "deu", 
   "lines": [
    "die wir jetzt sehen, "
   ]
  }, 
  {
   "begin": "263.000", 
   "children": [], 
   "end": "264.080", 
   "id": "s000081", 
   "language": "deu", 
   "lines": [
