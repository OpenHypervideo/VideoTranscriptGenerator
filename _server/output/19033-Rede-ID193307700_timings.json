  }, 
  {
   "begin": "54.680", 
   "children": [], 
   "end": "54.960", 
   "id": "s000128", 
   "language": "deu", 
   "lines": [
    "Das hei\u00dft, "
   ]
  }, 
  {
   "begin": "54.960", 
   "children": [], 
   "end": "55.960", 
   "id": "s000129", 
   "language": "deu", 
   "lines": [
