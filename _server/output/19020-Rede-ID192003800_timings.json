   ]
  }, 
  {
   "begin": "64.080", 
   "children": [], 
   "end": "65.200", 
   "id": "s000029", 
   "language": "deu", 
   "lines": [
    "sich hier zu engagieren, "
   ]
  }, 
  {
   "begin": "65.200", 
   "children": [], 
   "end": "65.800", 
   "id": "s000030", 
   "language": "deu", 
   "lines": [
