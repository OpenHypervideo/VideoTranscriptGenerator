  }, 
  {
   "begin": "92.680", 
   "children": [], 
   "end": "92.960", 
   "id": "s000035", 
   "language": "deu", 
   "lines": [
    "Das, "
   ]
  }, 
  {
   "begin": "92.960", 
   "children": [], 
   "end": "94.160", 
   "id": "s000036", 
   "language": "deu", 
   "lines": [
