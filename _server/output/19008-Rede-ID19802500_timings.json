  }, 
  {
   "begin": "59.080", 
   "children": [], 
   "end": "59.760", 
   "id": "s000019", 
   "language": "deu", 
   "lines": [
    "Herr Theurer, "
   ]
  }, 
  {
   "begin": "59.760", 
   "children": [], 
   "end": "62.880", 
   "id": "s000020", 
   "language": "deu", 
   "lines": [
