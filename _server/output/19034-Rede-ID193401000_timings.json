  }, 
  {
   "begin": "247.400", 
   "children": [], 
   "end": "248.120", 
   "id": "s000119", 
   "language": "deu", 
   "lines": [
    "dass sie sagen: "
   ]
  }, 
  {
   "begin": "248.120", 
   "children": [], 
   "end": "249.360", 
   "id": "s000120", 
   "language": "deu", 
   "lines": [
    "Ich riskiere es nicht, "
   ]
  }, 
  {
   "begin": "249.360", 
   "children": [], 
   "end": "251.040", 
   "id": "s000121", 
   "language": "deu", 
   "lines": [
