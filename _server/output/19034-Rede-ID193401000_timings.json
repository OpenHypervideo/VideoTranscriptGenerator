  }, 
  {
   "begin": "235.480", 
   "children": [], 
   "end": "238.640", 
   "id": "s000112", 
   "language": "deu", 
   "lines": [
    "Zum Thema Br\u00fcckenteilzeit sage ich: "
   ]
  }, 
  {
   "begin": "238.640", 
   "children": [], 
   "end": "239.040", 
   "id": "s000113", 
   "language": "deu", 
   "lines": [
    "Ja, "
   ]
  }, 
  {
   "begin": "239.040", 
   "children": [], 
   "end": "239.720", 
   "id": "s000114", 
   "language": "deu", 
   "lines": [
    "es ist gut, "
   ]
  }, 
  {
   "begin": "239.720", 
   "children": [], 
   "end": "241.440", 
   "id": "s000115", 
   "language": "deu", 
   "lines": [
