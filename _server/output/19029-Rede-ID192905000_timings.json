  }, 
  {
   "begin": "11.080", 
   "children": [], 
   "end": "12.960", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "Der Antrag der FDP, "
   ]
  }, 
  {
   "begin": "12.960", 
   "children": [], 
   "end": "15.600", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
