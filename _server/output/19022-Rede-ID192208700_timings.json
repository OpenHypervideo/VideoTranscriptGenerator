  }, 
  {
   "begin": "187.120", 
   "children": [], 
   "end": "188.120", 
   "id": "s000065", 
   "language": "deu", 
   "lines": [
    "Unser Europa, "
   ]
  }, 
  {
   "begin": "188.120", 
   "children": [], 
   "end": "189.520", 
   "id": "s000066", 
   "language": "deu", 
   "lines": [
    "das ist unser Zuhause. "
   ]
  }, 
  {
   "begin": "189.520", 
   "children": [], 
   "end": "190.560", 
   "id": "s000067", 
   "language": "deu", 
   "lines": [
    "Unser Europa, "
   ]
  }, 
  {
   "begin": "190.560", 
   "children": [], 
   "end": "192.080", 
   "id": "s000068", 
   "language": "deu", 
   "lines": [
