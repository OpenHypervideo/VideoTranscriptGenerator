   ]
  }, 
  {
   "begin": "65.640", 
   "children": [], 
   "end": "66.120", 
   "id": "s000016", 
   "language": "deu", 
   "lines": [
    "Sicher, "
   ]
  }, 
  {
   "begin": "66.120", 
   "children": [], 
   "end": "67.720", 
   "id": "s000017", 
   "language": "deu", 
   "lines": [
    "wir stimmen zu, "
   ]
  }, 
  {
   "begin": "67.720", 
   "children": [], 
   "end": "70.680", 
   "id": "s000018", 
   "language": "deu", 
   "lines": [
