  }, 
  {
   "begin": "224.800", 
   "children": [], 
   "end": "227.360", 
   "id": "s000087", 
   "language": "deu", 
   "lines": [
    "Uns sind 51\u00a0Boxer, "
   ]
  }, 
  {
   "begin": "227.360", 
   "children": [], 
   "end": "229.920", 
   "id": "s000088", 
   "language": "deu", 
   "lines": [
    "1\u00a0800\u00a0Gel\u00e4ndewagen und Lkw, "
   ]
  }, 
  {
   "begin": "229.920", 
   "children": [], 
   "end": "231.640", 
   "id": "s000089", 
   "language": "deu", 
   "lines": [
    "181\u00a0Puma, "
   ]
  }, 
  {
   "begin": "231.640", 
   "children": [], 
   "end": "233.280", 
   "id": "s000090", 
   "language": "deu", 
   "lines": [
    "28\u00a0NH90, "
   ]
  }, 
  {
   "begin": "233.280", 
   "children": [], 
   "end": "235.520", 
   "id": "s000091", 
   "language": "deu", 
   "lines": [
    "16\u00a0A400M, "
   ]
  }, 
  {
   "begin": "235.520", 
   "children": [], 
   "end": "239.320", 
   "id": "s000092", 
   "language": "deu", 
   "lines": [
