   ]
  }, 
  {
   "begin": "460.040", 
   "children": [], 
   "end": "464.760", 
   "id": "s000156", 
   "language": "deu", 
   "lines": [
    "Michael Link.\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "464.760", 
   "children": [], 
   "end": "466.720", 
   "id": "s000157", 
   "language": "deu", 
   "lines": [
    "N\u00e4chster Redner f\u00fcr die Fraktion Die Linke: "
   ]
  }, 
  {
   "begin": "466.720", 
   "children": [], 
   "end": "469.000", 
