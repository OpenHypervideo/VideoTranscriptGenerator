  }, 
  {
   "begin": "75.920", 
   "children": [], 
   "end": "76.680", 
   "id": "s000024", 
   "language": "deu", 
   "lines": [
    "Herr Brandner; "
   ]
  }, 
  {
   "begin": "76.680", 
   "children": [], 
   "end": "77.560", 
   "id": "s000025", 
   "language": "deu", 
   "lines": [
