  }, 
  {
   "begin": "86.640", 
   "children": [], 
   "end": "87.760", 
   "id": "s000028", 
   "language": "deu", 
   "lines": [
    "denn\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "87.760", 
   "children": [], 
   "end": "90.960", 
   "id": "s000029", 
   "language": "deu", 
   "lines": [
