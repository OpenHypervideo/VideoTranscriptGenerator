  }, 
  {
   "begin": "232.720", 
   "children": [], 
   "end": "233.680", 
   "id": "s000078", 
   "language": "deu", 
   "lines": [
    "im Irak, "
   ]
  }, 
  {
   "begin": "233.680", 
   "children": [], 
   "end": "235.680", 
   "id": "s000079", 
   "language": "deu", 
   "lines": [
