  }, 
  {
   "begin": "208.480", 
   "children": [], 
   "end": "210.200", 
   "id": "s000067", 
   "language": "deu", 
   "lines": [
    "das Hausrecht des St\u00e4rkeren, "
   ]
  }, 
  {
   "begin": "210.200", 
   "children": [], 
   "end": "215.360", 
   "id": "s000068", 
   "language": "deu", 
   "lines": [
