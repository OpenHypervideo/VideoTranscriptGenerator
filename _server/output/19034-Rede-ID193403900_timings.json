   "begin": "5.200", 
   "children": [], 
   "end": "6.840", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
    "Meine Damen und Herren! "
   ]
  }, 
  {
   "begin": "6.840", 
   "children": [], 
   "end": "8.280", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "Lassen Sie es mich kurzfassen: "
   ]
  }, 
  {
   "begin": "8.280", 
   "children": [], 
   "end": "9.040", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
    "Herr Scholz, "
   ]
  }, 
  {
   "begin": "9.040", 
   "children": [], 
   "end": "10.760", 
   "id": "s000005", 
   "language": "deu", 
   "lines": [
