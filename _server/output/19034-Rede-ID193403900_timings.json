  }, 
  {
   "begin": "179.560", 
   "children": [], 
   "end": "181.080", 
   "id": "s000087", 
   "language": "deu", 
   "lines": [
    "Kommen wir zur CSU, "
   ]
  }, 
  {
   "begin": "181.080", 
   "children": [], 
   "end": "181.240", 
   "id": "s000088", 
   "language": "deu", 
   "lines": [
    "die, "
   ]
  }, 
  {
   "begin": "181.240", 
   "children": [], 
   "end": "183.920", 
   "id": "s000089", 
   "language": "deu", 
   "lines": [
    "vertreten durch Herrn Seehofer, "
