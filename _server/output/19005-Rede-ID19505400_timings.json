  }, 
  {
   "begin": "144.960", 
   "children": [], 
   "end": "147.600", 
   "id": "s000045", 
   "language": "deu", 
   "lines": [
    "Jeder Selbstst\u00e4ndige muss Belege sammeln, "
   ]
  }, 
  {
   "begin": "147.600", 
   "children": [], 
   "end": "149.720", 
   "id": "s000046", 
   "language": "deu", 
   "lines": [
    "jeder Angestellte muss Erkl\u00e4rungen abgeben, "
   ]
  }, 
  {
   "begin": "149.720", 
   "children": [], 
   "end": "151.280", 
   "id": "s000047", 
   "language": "deu", 
   "lines": [
    "Nachweise erbringen. "
   ]
  }, 
  {
   "begin": "151.280", 
   "children": [], 
   "end": "156.280", 
   "id": "s000048", 
   "language": "deu", 
   "lines": [
    "Sie k\u00f6nnen 4\u00a0300\u00a0Euro steuerfrei vereinnahmen\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "156.280", 
   "children": [], 
   "end": "158.440", 
   "id": "s000049", 
   "language": "deu", 
   "lines": [
