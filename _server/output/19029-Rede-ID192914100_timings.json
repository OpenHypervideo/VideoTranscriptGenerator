   ]
  }, 
  {
   "begin": "261.880", 
   "children": [], 
   "end": "262.600", 
   "id": "s000115", 
   "language": "deu", 
   "lines": [
    "ist mir schleierhaft; "
   ]
  }, 
  {
   "begin": "262.600", 
   "children": [], 
   "end": "264.680", 
   "id": "s000116", 
   "language": "deu", 
   "lines": [
