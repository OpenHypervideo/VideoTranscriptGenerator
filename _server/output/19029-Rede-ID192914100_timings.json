  {
   "begin": "307.280", 
   "children": [], 
   "end": "308.680", 
   "id": "s000138", 
   "language": "deu", 
   "lines": [
    "Wer stimmt dagegen?\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "308.680", 
   "children": [], 
   "end": "309.720", 
   "id": "s000139", 
   "language": "deu", 
   "lines": [
