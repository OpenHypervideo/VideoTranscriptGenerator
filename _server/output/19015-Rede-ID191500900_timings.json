  }, 
  {
   "begin": "315.560", 
   "children": [], 
   "end": "316.480", 
   "id": "s000107", 
   "language": "deu", 
   "lines": [
    "wohlgemerkt; "
   ]
  }, 
  {
   "begin": "316.480", 
   "children": [], 
   "end": "319.040", 
   "id": "s000108", 
   "language": "deu", 
   "lines": [
