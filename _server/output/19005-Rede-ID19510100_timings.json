  }, 
  {
   "begin": "152.000", 
   "children": [], 
   "end": "152.520", 
   "id": "s000047", 
   "language": "deu", 
   "lines": [
    "Sie wissen, "
   ]
  }, 
  {
   "begin": "152.520", 
   "children": [], 
   "end": "153.880", 
   "id": "s000048", 
   "language": "deu", 
   "lines": [
