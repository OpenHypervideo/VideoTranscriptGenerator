   ]
  }, 
  {
   "begin": "4.280", 
   "children": [], 
   "end": "7.920", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
    "Frau Pr\u00e4sidentin.\u00a0\u2013 "
