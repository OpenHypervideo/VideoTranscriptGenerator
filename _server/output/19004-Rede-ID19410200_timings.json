  }, 
  {
   "begin": "85.040", 
   "children": [], 
   "end": "86.400", 
   "id": "s000031", 
   "language": "deu", 
   "lines": [
    "bei der Saat, "
   ]
  }, 
  {
   "begin": "86.400", 
   "children": [], 
   "end": "87.240", 
   "id": "s000032", 
   "language": "deu", 
   "lines": [
    "bei der Ernte, "
   ]
  }, 
  {
   "begin": "87.240", 
   "children": [], 
   "end": "88.240", 
   "id": "s000033", 
   "language": "deu", 
   "lines": [
