  }, 
  {
   "begin": "193.000", 
   "children": [], 
   "end": "194.720", 
   "id": "s000057", 
   "language": "deu", 
   "lines": [
    "die in pakistanischer Haft sitzt, "
   ]
  }, 
  {
   "begin": "194.720", 
   "children": [], 
   "end": "202.040", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
