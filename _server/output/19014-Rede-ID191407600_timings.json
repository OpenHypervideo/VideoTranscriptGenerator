  }, 
  {
   "begin": "99.480", 
   "children": [], 
   "end": "100.200", 
   "id": "s000027", 
   "language": "deu", 
   "lines": [
    "stellt fest, "
   ]
  }, 
  {
   "begin": "100.200", 
   "children": [], 
   "end": "106.600", 
   "id": "s000028", 
   "language": "deu", 
   "lines": [
