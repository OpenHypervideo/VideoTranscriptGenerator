   ]
  }, 
  {
   "begin": "34.120", 
   "children": [], 
   "end": "34.920", 
   "id": "s000010", 
   "language": "deu", 
   "lines": [
    "Zitat\u00a0\u2013: "
   ]
  }, 
  {
   "begin": "34.920", 
   "children": [], 
   "end": "38.720", 
   "id": "s000011", 
   "language": "deu", 
   "lines": [
