   "end": "0.320", 
   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Herr Pr\u00e4sident! "
   ]
  }, 
  {
   "begin": "0.320", 
   "children": [], 
   "end": "1.160", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
    "Meine Damen und Herren! "
   ]
  }, 
  {
   "begin": "1.160", 
   "children": [], 
   "end": "13.520", 
   "id": "s000003", 
   "language": "deu", 
   "lines": [
    "Dem vorliegenden Antrag liegen sehr viele ehrenvolle Motive zugrunde: "
   ]
  }, 
  {
   "begin": "13.520", 
   "children": [], 
   "end": "15.480", 
   "id": "s000004", 
   "language": "deu", 
   "lines": [
    "unsere Partner im Mittelmeer unterst\u00fctzen, "
   ]
  }, 
  {
   "begin": "15.480", 
   "children": [], 
   "end": "17.160", 
   "id": "s000005", 
   "language": "deu", 
   "lines": [
    "Terror bek\u00e4mpfen, "
   ]
  }, 
  {
   "begin": "17.160", 
   "children": [], 
   "end": "18.800", 
   "id": "s000006", 
   "language": "deu", 
   "lines": [
    "Menschenschmuggel eind\u00e4mmen, "
   ]
  }, 
  {
   "begin": "18.800", 
   "children": [], 
   "end": "20.640", 
   "id": "s000007", 
   "language": "deu", 
   "lines": [
