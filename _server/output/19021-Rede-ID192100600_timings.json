  }, 
  {
   "begin": "150.560", 
   "children": [], 
   "end": "151.760", 
   "id": "s000059", 
   "language": "deu", 
   "lines": [
    "Wir reden \u00fcber Menschen, "
   ]
  }, 
  {
   "begin": "151.760", 
   "children": [], 
   "end": "154.560", 
   "id": "s000060", 
   "language": "deu", 
   "lines": [
