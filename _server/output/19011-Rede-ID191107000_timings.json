  }, 
  {
   "begin": "268.880", 
   "children": [], 
   "end": "270.440", 
   "id": "s000084", 
   "language": "deu", 
   "lines": [
    "Frau Kollegin Werner.\u00a0\u2013 "
   ]
  }, 
  {
