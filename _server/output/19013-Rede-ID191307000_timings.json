  }, 
  {
   "begin": "163.000", 
   "children": [], 
   "end": "163.680", 
   "id": "s000067", 
   "language": "deu", 
   "lines": [
    "die geplant war, "
   ]
  }, 
  {
   "begin": "163.680", 
   "children": [], 
   "end": "165.360", 
   "id": "s000068", 
   "language": "deu", 
   "lines": [
