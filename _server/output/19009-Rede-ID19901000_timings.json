  }, 
  {
   "begin": "325.120", 
   "children": [], 
   "end": "327.280", 
   "id": "s000098", 
   "language": "deu", 
   "lines": [
    "Herr Gauland: "
   ]
  }, 
  {
   "begin": "327.280", 
   "children": [], 
   "end": "333.600", 
   "id": "s000099", 
   "language": "deu", 
   "lines": [
