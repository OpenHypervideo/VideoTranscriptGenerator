  {
   "begin": "203.200", 
   "children": [], 
   "end": "204.760", 
   "id": "s000090", 
   "language": "deu", 
   "lines": [
    "Sie fordern in Ihrem Antrag, "
   ]
  }, 
  {
   "begin": "204.760", 
   "children": [], 
   "end": "207.960", 
   "id": "s000091", 
   "language": "deu", 
   "lines": [
    "zum Beispiel die 48-Stunden-Woche, "
   ]
  }, 
  {
   "begin": "207.960", 
   "children": [], 
   "end": "209.440", 
   "id": "s000092", 
   "language": "deu", 
   "lines": [
    "die gesetzlich erlaubt ist, "
   ]
  }, 
  {
   "begin": "209.440", 
