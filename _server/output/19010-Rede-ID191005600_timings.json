  }, 
  {
   "begin": "149.800", 
   "children": [], 
   "end": "150.280", 
   "id": "s000058", 
   "language": "deu", 
   "lines": [
    "das hei\u00dft, "
   ]
  }, 
  {
   "begin": "150.280", 
   "children": [], 
   "end": "150.960", 
   "id": "s000059", 
   "language": "deu", 
   "lines": [
    "die Gelder, "
   ]
  }, 
  {
   "begin": "150.960", 
   "children": [], 
   "end": "152.400", 
   "id": "s000060", 
   "language": "deu", 
   "lines": [
