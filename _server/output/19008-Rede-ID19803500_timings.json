  }, 
  {
   "begin": "44.480", 
   "children": [], 
   "end": "44.880", 
   "id": "s000019", 
   "language": "deu", 
   "lines": [
    "Herr Otte, "
   ]
  }, 
  {
   "begin": "44.880", 
   "children": [], 
   "end": "47.240", 
   "id": "s000020", 
   "language": "deu", 
   "lines": [
    "es hier zum Beispiel tun\u00a0\u2013, "
   ]
  }, 
  {
   "begin": "47.240", 
   "children": [], 
   "end": "49.120", 
   "id": "s000021", 
   "language": "deu", 
   "lines": [
