  }, 
  {
   "begin": "12.320", 
   "children": [], 
   "end": "13.480", 
   "id": "s000006", 
   "language": "deu", 
   "lines": [
    "Wir stehen, "
   ]
  }, 
  {
   "begin": "13.480", 
   "children": [], 
   "end": "15.240", 
   "id": "s000007", 
   "language": "deu", 
   "lines": [
