  }, 
  {
   "begin": "225.560", 
   "children": [], 
   "end": "228.240", 
   "id": "s000088", 
   "language": "deu", 
   "lines": [
    "n\u00e4mlich eine Sicherheitspolitik, "
   ]
  }, 
  {
   "begin": "228.240", 
