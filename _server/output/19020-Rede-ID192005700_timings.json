  }, 
  {
   "begin": "244.280", 
   "children": [], 
   "end": "245.000", 
   "id": "s000080", 
   "language": "deu", 
   "lines": [
    "Ich glaube, "
   ]
  }, 
  {
   "begin": "245.000", 
   "children": [], 
   "end": "247.200", 
   "id": "s000081", 
   "language": "deu", 
   "lines": [
