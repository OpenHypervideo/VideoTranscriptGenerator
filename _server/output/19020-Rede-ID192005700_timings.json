   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Sehr geehrter Herr Pr\u00e4sident! "
   ]
  }, 
  {
   "begin": "6.480", 
   "children": [], 
   "end": "8.640", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
