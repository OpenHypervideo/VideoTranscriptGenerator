   ]
  }, 
  {
   "begin": "271.440", 
   "children": [], 
   "end": "273.560", 
   "id": "s000084", 
   "language": "deu", 
   "lines": [
    "nicht den Gesch\u00e4ftemachern, "
   ]
  }, 
  {
   "begin": "273.560", 
   "children": [], 
   "end": "276.840", 
   "id": "s000085", 
   "language": "deu", 
   "lines": [
    "sondern den Ehrenamtlern. "
   ]
  }, 
  {
   "begin": "276.840", 
   "children": [], 
   "end": "278.600", 
   "id": "s000086", 
   "language": "deu", 
   "lines": [
