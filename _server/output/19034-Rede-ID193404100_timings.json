   ]
  }, 
  {
   "begin": "42.840", 
   "children": [], 
   "end": "43.320", 
   "id": "s000025", 
   "language": "deu", 
   "lines": [
    "empfehle ich, "
   ]
  }, 
  {
   "begin": "43.320", 
   "children": [], 
   "end": "44.520", 
   "id": "s000026", 
   "language": "deu", 
   "lines": [
