  }, 
  {
   "begin": "167.960", 
   "children": [], 
   "end": "169.080", 
   "id": "s000092", 
   "language": "deu", 
   "lines": [
    "Der Kollege Steinbr\u00fcck\u00a0\u2013 "
   ]
  }, 
  {
   "begin": "169.080", 
   "children": [], 
