{
 "fragments": [
  {
   "begin": "0.000", 
   "children": [], 
   "end": "1.480", 
   "id": "s000001", 
   "language": "deu", 
   "lines": [
    "Sehr geehrte Frau Pr\u00e4sidentin! "
   ]
  }, 
  {
   "begin": "1.480", 
   "children": [], 
   "end": "3.600", 
   "id": "s000002", 
   "language": "deu", 
   "lines": [
