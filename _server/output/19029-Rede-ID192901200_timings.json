  }, 
  {
   "begin": "371.960", 
   "children": [], 
   "end": "372.640", 
   "id": "s000137", 
   "language": "deu", 
   "lines": [
    "SPD, "
   ]
  }, 
  {
   "begin": "372.640", 
   "children": [], 
   "end": "373.120", 
   "id": "s000138", 
   "language": "deu", 
   "lines": [
    "AfD, "
